{
  "name": "node-js-api-seed",
  "version": "1.0.0",
  "description": "Node js api seed (nodeJS + express + mocha + docker + awilix)",
  "main": "server.js",
  "author": "Mauricio Minella",
  "scripts": {
    "waitredis": "node bin/waitredis.js",
    "waitmongo": "node bin/waitmongo.js",
    "lint": "eslint src/ spec",
    "lint-fix": "eslint '**/*.js' --fix",
    "start": "npm run waitredis && npm run waitmongo; node ./bin/service.js",
    "test-integration": "npm run waitredis && npm run waitmongo; mocha --opts spec/integration/mocha.opts --delay --recursive --watch-extensions .spec.js ./spec/integration",
    "test-unit": "mocha --opts spec/unit/mocha.opts --recursive --watch-extensions .spec.js ./spec/unit",
    "test": "npm run test-unit && npm run test:integration",
    "cover": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --recursive ./spec/unit/",
    "test:unit": "mocha --compilers js:babel-register --timeout 30000 --report spec --recursive --watch-extensions .spec.js ./spec/unit",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:integration": "docker-compose run --rm test; docker rm -f $(docker ps --filter 'name=nodejsapiseed*' -qa)",
    "build": "babel src/ --out-dir lib",
    "dev": "babel-node bin/service.js",
    "worker": "babel-node bin/worker.js",
    "load-data": "babel-node bin/dataloader.js",
    "dev:watch": "supervisor --watch src -- -r 'babel-register' ./bin/service.js",
    "up": "docker-compose up local",
    "down": "docker rm -f $(docker ps --filter 'name=nodejsapiseed*' -qa)"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "babel": {
    "plugins": [
      "transform-es2015-destructuring",
      "transform-es2015-parameters",
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator",
      "transform-object-rest-spread",
      "syntax-object-rest-spread"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "amqp": "^0.2.6",
    "amqplib": "^0.5.1",
    "awilix": "^2.2.3",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-es2015-parameters": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "body-parser": "^1.15.2",
    "chai-as-promised": "^6.0.0",
    "cors": "^2.7.1",
    "decompress": "^4.0.0",
    "event-to-promise": "^0.7.0",
    "express": "^4.14.0",
    "line-by-line": "^0.1.5",
    "meaning-error": "^2.1.0",
    "meaning-error-middleware": "^0.1.1",
    "mongodb": "^2.1.18",
    "n-readlines": "^0.2.7",
    "promise": "^7.1.1",
    "request": "^2.79.0",
    "rsmq": "^0.7.2",
    "rsmq-worker": "^0.5.2",
    "sleep": "^5.1.0",
    "underscore": "^1.8.3",
    "unique-filename": "^1.1.0",
    "url": "^0.11.0",
    "validator": "^6.2.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.10.1",
    "istanbul": "^0.4.5",
    "joi": "^10.2.2",
    "joi-assert": "0.0.3",
    "mocha": "^2.5.3",
    "nock": "^9.0.8",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "testdouble": "^1.11.2"
  }
}
